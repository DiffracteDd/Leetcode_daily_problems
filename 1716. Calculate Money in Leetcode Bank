int totalMoney(int n) {
    int sum=0;
    if(n==0)
    {
        return 0;
    }
    int x=1;
    int i=1;
    while(x<=n)
    {
        sum = sum +i;
        x++;
        int j=i+1;
        while(x<=n && j<=i+6)
        {
            sum = sum + j;
            j++;
            x++;
        }
        i++;
    }
    return sum;
}
   
